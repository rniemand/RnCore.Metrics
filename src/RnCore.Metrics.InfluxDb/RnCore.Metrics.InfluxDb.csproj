<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ProjectGuid>ED9365B8-702A-4D4D-884F-BD9DA57EC59B</ProjectGuid>
    <Company>Richard Niemand</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>Richard Niemand</Authors>
    <NeutralLanguage>en</NeutralLanguage>
    <Description>InfluxDB 2 compatible output for RnCore.Metrics</Description>
    <PackageProjectUrl>http://www.richardn.ca/RnCore.Metrics/#/</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/rniemand/RnCore.Metrics</RepositoryUrl>
    <AssemblyName>RnCore.Metrics.InfluxDb</AssemblyName>
    <Version>6.0.0.103</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="InfluxDB.Client" Version="4.9.0" />
    <PackageReference Include="RnCore.Metrics" Version="6.0.0.101" />
  </ItemGroup>

</Project>
